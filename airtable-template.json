      "fields": [
        {
          "name": "name",
          "type": "singleLineText",
          "description": "Nom du projet"
        },
        {
          "name": "description",
          "type": "multilineText",
          "description": "Description du projet"
        },
        {
          "name": "moduleId",
          "type": "multipleRecordLinks",
          "description": "Lien vers le module",
          "options": {
            "linkedTableId": "Modules"
          }
        },
        {
          "name": "deadline",
          "type": "date",
          "description": "Date limite du projet"
        },
        {
          "name": "studentIds",
          "type": "multipleRecordLinks",
          "description": "Étudiants assignés au projet",
          "options": {
            "linkedTableId": "Students"
          }
        },
        {
          "name": "status",
          "type": "singleSelect",
          "description": "Statut du projet",
          "options": {
            "choices": [
              {"name": "Non remis", "color": "red"},
              {"name": "Remis", "color": "blue"},
              {"name": "À corriger", "color": "orange"},
              {"name": "Validé", "color": "green"}
            ]
          }
        },
        {
          "name": "trackingType",
          "type": "singleSelect",
          "description": "Type de suivi",
          "options": {
            "choices": [
              {"name": "simple", "color": "blue"},
              {"name": "step_by_step", "color": "purple"}
            ]
          }
        },
        {
          "name": "stepStatus",
          "type": "singleSelect",
          "description": "Statut de l'étape",
          "options": {
            "choices": [
              {"name": "Point d'étape 1", "color": "blue"},
              {"name": "Point d'étape 2", "color": "blue"},
              {"name": "Validation des dates des restitutions", "color": "orange"},
              {"name": "Remise des travaux", "color": "green"}
            ]
          }
        },
        {
          "name": "submissionDate",
          "type": "date",
          "description": "Date de soumission"
        },
        {
          "name": "grade",
          "type": "number",
          "description": "Note du projet"
        },
        {
          "name": "comments",
          "type": "multilineText",
          "description": "Commentaires sur le projet"
        }
